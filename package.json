{
  "name": "iot-core-gen2",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "debug": "npm run db && node --trace-warnings main.js",
    "prod": "npm run db && npm run serve",
    "db": "npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "serve": "nodemon --trace-warnings main.js",
    "pm2": "pm2 stop processes.json && pm2 delete processes.json && pm2 start processes.json",
    "lint": "prettier -c . && eslint --ignore-path .gitignore .",
    "lint:fix": "prettier --write . && eslint --ignore-path .gitignore . --fix",
    "lint-fix": "prettier --write . && eslint --ignore-path .gitignore . --fix",
    "db-undo": "npx sequelize-cli db:migrate:undo",
    "test-db": "npx sequelize-cli db:create && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "pretest": "cross-env NODE_ENV=test npm run test-db",
    "test": "cross-env NODE_ENV=test jest  --runInBand --forceExit",
    "test:watch": "cross-env NODE_ENV=test INFLUX_DB_NAME=test jest --watch",
    "pretest-without-db-script": "cross-env NODE_ENV=test",
    "test-without-db-script": "jest --forceExit",
    "deploy": "sh ./deploy.sh",
    "influx-setup": "node ./influx_setup.js"
  },
  "author": "Asfund",
  "license": "ISC",
